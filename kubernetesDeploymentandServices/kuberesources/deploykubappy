apiVersion: apps/v1
kind: Deployment
metadata:
  name: nginx-deployment
  namespace: mywebapp
  labels:
    app: nginx
spec:
  replicas: 3
  selector:
    matchLabels:
      app: nginx
  template:
    metadata:
      labels:
        app: nginx
    spec:
      containers:
      - name: nginx
        image: nginx:1.14.2
       # imagePullPolicy: Never
        ports:
        - containerPort: 80
        resources:
          requests:
            memory: "100Mi"
            cpu:     "250m"
          limits:  
             cpu: "300m"
             memory: "100Mi"
